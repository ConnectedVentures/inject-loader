  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel -d dist src/",
    "pretest": "npm run build",
    "test:old": "mocha --compilers coffee:coffee-script/register",
    "test": "flow && jest",
    "prepublish": "npm run build"
  },
  "files": [
    "*.md",
    "dist"
  ],
  "author": "Justin Morris <desk@pixelbloom.com> (http://pixelbloom.com)",
  "repository": {
    "type": "git",
    "url": "git@github.com:plasticine/inject-loader.git"
  },
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "flow-bin": "^0.36.0",
    "jest": "^17.0.3"
  },
  "keywords": [
    "webpack",
